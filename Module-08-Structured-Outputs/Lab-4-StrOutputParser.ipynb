    {
      "cell_type": "code",
      "source": [
        "from langchain_core.prompts import PromptTemplate\n",
        "from langchain_openai import ChatOpenAI\n",
        "from langchain_core.output_parsers import StrOutputParser"
      ],
      "metadata": {
        "id": "bb81lUv_-dLd"
      },
      "execution_count": 11,
      "outputs": []
    },
    {
      "cell_type": "code",
